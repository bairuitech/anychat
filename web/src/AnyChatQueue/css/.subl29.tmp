}

#segoeicon {
    font-size: 80px;
    font-family: "Segoe UI Symbol";
    font-weight: bold;
    color: #1f1f1f;
    margin: 0px 24px 0px 10px;
    display: block;
}

#erroricon {
    font-size: 80px;
    font-family: "Browser MDL2 Assets";
    font-weight: bold;
    color: #a90000;
    margin: 0px 24px 0px 10px;
    display: block;
}

#imageicon {
    height: 150px;
    width: 100px;
    margin: 0px 24px 0px 10px;
    display: block;
}

#smallerroricon {
    font-size: 15px;
    font-family: "Browser MDL2 Assets";
    font-weight: bold;
    color: #a90000;
    display: block;
}

#smallgoodicon {
    font-size: 15px;
    font-family: "Browser MDL2 Assets";
    font-weight: bold;
    color: #5c9100;
    display: block;
}

.errorPage {
    display: none;
}

.text-body {
    line-height: 20px;
    font-size: 15px;
    font-family: "Microsoft YaHei UI";
    font-weight: normal;
    color: #1f1f1f;
    margin: 0;
    padding: 0;
}

.button-row {
    width: 480px;
    height: 32px;
    margin: 36px 0 42px 120px;
    padding: 0 0 0 0;
    text-align: left;
}
button, input[type=button], input[type=reset], input[type=submit] {
    display: inline-block;
}

.pushbutton, button, input[type=button], input[type=reset], input[type=submit] {
