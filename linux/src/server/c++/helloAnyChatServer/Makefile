CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS =  -Wall -fexceptions
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

aarch64 =
ifeq ($(aarch64), 1)
	CC = aarch64-linux-gnu-gcc 
	CXX = aarch64-linux-gnu-g++ 
	AR = aarch64-linux-gnu-ar
	LD = aarch64-linux-gnu-g++
endif

INC_RELEASE =  $(INC) -I../../../../sdk/server/c++
CFLAGS_RELEASE =  $(CFLAGS) -O2
RESINC_RELEASE =  $(RESINC)
RCFLAGS_RELEASE =  $(RCFLAGS)
LIBDIR_RELEASE =  $(LIBDIR) -L../../../../bin/serversdk
LIB_RELEASE = $(LIB) -lanychatserversdk -lz -ldl -lrt
LDFLAGS_RELEASE =  $(LDFLAGS) -s -Wl,-rpath .
OBJDIR_RELEASE = obj/Release
DEP_RELEASE = 
OUT_RELEASE = ../../../../bin/serversdk/helloAnyChatServer_c++

OBJ_RELEASE = $(OBJDIR_RELEASE)/main.o

all: Release

clean: clean_Release

before_Release: 
	test -d ../../../../bin/server || mkdir -p ../../../../bin/server
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)

after_Release: 

Release: before_Release out_Release after_Release

out_Release: $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) $(LDFLAGS_RELEASE) $(LIBDIR_RELEASE) $(OBJ_RELEASE) $(LIB_RELEASE) -o $(OUT_RELEASE)

$(OBJDIR_RELEASE)/main.o: main.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c main.cpp -o $(OBJDIR_RELEASE)/main.o

clean_Release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf ../../../../bin/server/helloAnyChatServer

.PHONY: before_Release after_Release clean_Release

